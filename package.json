{
	"name": "codeui",
	"displayName": "codeUI",
	"description": "Do shit to the ui",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "elementProvider.editValue",
				"title": "Edit Value",
				"arguments": "[]"
			},
			{
				"command": "getCustomizedElements",
				"title": "Get customized elements TEST"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "elementTreeView",
					"title": "codeUI",
					"icon": "paintbrush.png"
				}
			]
		},
		"views": {
			"elementTreeView": [
				{
					"id": "elementTreeView",
					"name": "Interface Elements"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"jsonc-parser": "^2.1.0"
	}
}
